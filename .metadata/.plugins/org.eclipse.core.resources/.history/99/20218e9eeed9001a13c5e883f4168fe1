const vm = new Vue({
	el: "#app",
	vuetify: new Vuetify(),
	data: {
		valid: true,
		emailRules: [
	      v => !!v || 'E-mail é obrigatório',
	    ],
	 	senhaRules: [
	      v => !!v || 'E-mail é obrigatório',
	    ],
	    
	    dadosUsuario: {
	    	email: '',
	    	senha: '',
	    	manter_conectado: false,
	    }
	},
	methods: {
		validate() {
			this.$refs.form.validate()
		}
	}
})