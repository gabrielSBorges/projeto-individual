import telas from './telas.js'

let routes = []

telas.map(tela => {
	const { path, component } = tela
	
	routes.push({ path, component })
})

const router = new VueRouter({
	routes
})

var route = new Vue({
	router,
	el: '#app',
	vuetify: new Vuetify(),
	data: {
		
	},
	methods: {
		
	}
}).$mount('#app')